generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model user{
  id        Int   @id @default(autoincrement()) @db.UnsignedInt
  username  String @unique
  password  String
  email     String
  createdAt DateTime  @default(now()) @updatedAt
}

